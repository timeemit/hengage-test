= form_for @time_block do |f|
  - if @time_block.errors.any?
    #error_explanation
      %h2= "#{pluralize(@time_block.errors.count, "error")} prohibited this time_block from being saved:"
      %ul
        - @time_block.errors.full_messages.each do |msg|
          %li= msg

  .actions
    = f.submit 'Save'
